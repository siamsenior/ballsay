function getPlayerSource(autostart){
    $.ajax({
        url: 'https://livescorethai.net/player/source',
        type: 'GET',
        data: { live_source_video: $(".btn-switch-channel.active").data('channel') },
        dataType:'json',
        success:function(data){
            var iOS = /iPad|iPhone|iPod/.test(navigator.platform || "");
            var isSafari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent);
           
            
            if(isSafari){
                jwplayer("player").setup({skin:{name: "seven"},mute:false,playlist: [{ file: data.stream_url , image:""}],  width: "100%", aspectratio: "16:9", autostart:false, requestTimeout:15000, cast:{} });
            }else{
                //jwplayer("player").setup({skin:{name: "seven"},mute:false,playlist: [{ file: data.stream_url , image:""}],  width: "100%", aspectratio: "16:9", autostart:false, requestTimeout:15000, cast:{} });
                
                const video = document.querySelector('video');
                const player = new Plyr(video, {captions: {active: true, update: true, language: 'en'}});
                if (!Hls.isSupported()) {
                    video.src = data.stream_url;
                } else {
                    const hls = new Hls();
                    hls.loadSource(data.stream_url);
                    hls.attachMedia(video);
                    window.hls = hls;
                    player.on('languagechange', () => {
                        setTimeout(() => hls.subtitleTrack = player.currentTrack, 50);
                    });
                }
            }
            

    }
    });
}

function matchStats(id){
    
    $.ajax({
        url:base_url+"/match/stats/"+id,
        success:function(res){
            var current_tab = $(".nav-item.active").index();
            $("#match-stats").html(res);
            $(".nav-item").removeClass("active")
            $(".nav-item").eq(current_tab).addClass("active")
            $(".tab-pane").removeClass("show active")
            $(".tab-pane").eq(current_tab).addClass("show active")
        }
    })
}
