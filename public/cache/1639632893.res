    $('body').on('click','.btn-favorite',function(e){
        e.preventDefault();
        var saved_fav_list = JSON.parse(localStorage.getItem('fav_list'));
       
        if(!$(this).hasClass('added')){
            $(this).addClass('added')
            if(saved_fav_list!==null){
                saved_fav_list.push($(this).data('id'));
                localStorage.setItem('fav_list', JSON.stringify(saved_fav_list));
            }else{
                var fav_list = new Array();
                fav_list.push($(this).data('id'));
                localStorage.setItem('fav_list', JSON.stringify(fav_list));
            }
        }else{
            var index = saved_fav_list.indexOf($(this).data('id'));
            if (index !== -1) saved_fav_list.splice(index, 1);
            localStorage.setItem('fav_list', JSON.stringify(saved_fav_list));
            $(this).removeClass('added')
        }
        var saved_fav_list = JSON.parse(localStorage.getItem('fav_list'));
        if(saved_fav_list!==null){
            if(saved_fav_list.length>0){
                $('.badge-favorite').text(saved_fav_list.length).removeClass('hidden')
            }else{
                $('.badge-favorite').text("0").addClass('hidden')
            }
        }
    });
    var base_url = "https://livescorethai.net";
                if(!window.location.hash && window.location.hash!="#live") {
            getLiveScore($('.days-tab li a.active').data('date'));
        }
        setInterval(function(){
            getLiveScore($('.days-tab li a.active').data('date'));
        },15000)
        function getLiveScore(date){
            $.ajax({
                url:base_url+"/livescore/"+date,
                success:function(res){
                    $("#livescore-table tbody").html(res);
                    $('.loading-overlay').hide();
                    setFavstar()
                    if(window.location.hash && window.location.hash=="#live") {
                        $(".days-tab li a").removeClass('active');
                        $(".days-tab li a:first").addClass('active');
                        $('.match:not(.livematch)').hide();
                        $(".league-header").hide();
                        $('.livematch').each(function(){
                            $(".league-header.league-"+$(this).attr('data-l-id')).show();
                        });
                    }
                    if($(".livescore-filter-tab a.active").attr('href')=="#favorite"){
                        $('.btn-favorite:not(.added)').parent().parent().hide();
                        $(".league-header").hide();
                        $('.btn-favorite.added').each(function(){
                            $(".league-header.league-"+$(this).parent().parent().attr('data-l-id')).show();
                        });
                    }
                }
            })
        }

        
    if(window.location.hash && window.location.hash=="#live") {
        $(".live-btn").trigger('click');
        getLiveScore('now');
    }
    $(window).on('hashchange', function() {
